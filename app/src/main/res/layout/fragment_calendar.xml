<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragments.Calendar_fragment">


    <ImageView
        android:id="@+id/imageButton4"
        android:layout_width="58dp"
        android:layout_height="49dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="10dp"
        android:orientation="vertical"
        android:rotation="180"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/new_task_toolbar"
        app:srcCompat="@drawable/baseline_navigate_next_black_48" />

    <ImageView
        android:id="@+id/imageButton5"
        android:layout_width="58dp"
        android:layout_height="49dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/new_task_toolbar"
        app:srcCompat="@drawable/baseline_navigate_next_black_48" />

    <TextView
        android:id="@+id/imageButton6"
        android:layout_width="259dp"
        android:layout_height="49dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:padding="-10dp"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toStartOf="@+id/imageButton5"
        app:layout_constraintStart_toEndOf="@+id/imageButton4"
        app:layout_constraintTop_toBottomOf="@+id/new_task_toolbar"
        app:srcCompat="@android:drawable/divider_horizontal_textfield" />

    <com.google.android.material.appbar.MaterialToolbar
        android:id="@+id/new_task_toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:menu="@menu/calendar_toolbar_menu"
        app:subtitle="Kalendar" />
</androidx.constraintlayout.widget.ConstraintLayout>